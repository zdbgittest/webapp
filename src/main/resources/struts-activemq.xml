<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="activemq" extends="struts-default" namespace="/activemq">
		<action name="operList" class="activemqAction" method="operList">
			<result name="success">/WEB-INF/templates/activemq/list.jsp</result>
		</action>		
		<action name="putInQuene" class="activemqAction" method="putInQuene"></action>
		<action name="getOutQuene" class="activemqAction" method="getOutQuene"></action>
	</package>
</struts>